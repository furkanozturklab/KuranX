                                </Grid>

                                <StackPanel Grid.Row="3" Grid.Column="0" Grid.ColumnSpan="4" Grid.RowSpan="2" Margin="20 5 20 5">
                                    <TextBlock Text="İsim Soyisim"  Style="{DynamicResource prf_editBoxText}" />
