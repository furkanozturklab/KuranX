                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="32" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="32" />
                                        </Grid.RowDefinitions>

                                        <Button Grid.Column="0" Grid.Row="0" Tag="InformationVariant" Style="{DynamicResource pp_settingsIconBox}" />

                                        <TextBox x:Name="st_warningSecond" Style="{DynamicResource pp_settingsTextBox}" Grid.Column="1" Grid.Row="0" Text="" PreviewTextInput="st_PreviewTextInput" />
                                    </Grid>

                                    <Label x:Name="st_warningSecondErr" Content="* Bildirimlerin Ekranda Kalma Süresi (Sn cinsinden) " Style="{DynamicResource pp_settingsInfo}" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Grid.Column="0" Grid.Row="7" Grid.RowSpan="2" Margin="10">
                                    <TextBlock Text="Sıfırlama"  Style="{DynamicResource pp_settingsHeader}" TextAlignment="Center" />
                                    <Grid Margin="0 10 0 0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="32" />
                                        </Grid.RowDefinitions>

                                        <Button Grid.Column="0" Grid.Row="0" Content="Verileri Sıfırla" Tag="ArrowCounterclockwise" Style="{DynamicResource pp_DefaultButtonBstrp}" />
                                        <Button Grid.Column="1" Grid.Row="0" Content="Hesabı Sil" Tag="TrashMD" Style="{DynamicResource pp_DefaultButtonSend}" />
                                    </Grid>

                                    <Label Content="! Bu işlemler kalıcı olduğunu unutmayınız " Style="{DynamicResource pp_settingsInfo}" Margin="0 10 0 0" />
                                </StackPanel>

                                <TextBlock Text="Hatırlatıcı Ayarları" Grid.Row="0" Grid.Column="1"   VerticalAlignment="Center" HorizontalAlignment="Center" FontSize="24" Foreground="#1C1E2F" FontFamily="{DynamicResource PoppinsMedium}" />

                                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Grid.Column="1" Grid.Row="1" Grid.RowSpan="2" Margin="10">
                                    <TextBlock Text="Hatırlatıcı Süresi"  Style="{DynamicResource pp_settingsHeader}" />
                                    <Grid Margin="0 5 0 0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="32" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="32" />
                                        </Grid.RowDefinitions>

                                        <Button Grid.Column="0" Grid.Row="0" Tag="Bell" Style="{DynamicResource pp_settingsIconBox}" />

                                        <TextBox x:Name="st_remiderTime" Style="{DynamicResource pp_settingsTextBox}" Grid.Column="1" Grid.Row="0" Text="" PreviewTextInput="st_PreviewTextInput" />
                                    </Grid>

                                    <Label x:Name="st_remiderTimeErr" Content="* Hatırlatıcıların Ekranda Kalma Süresi (Sn cinsinden) " Style="{DynamicResource pp_settingsInfo}" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical" VerticalAlignment="Center" Grid.Column="1" Grid.Row="3" Grid.RowSpan="2" Margin="10">
                                    <TextBlock Text="Hatırlatıcı Tekrarlama Süresi"  Style="{DynamicResource pp_settingsHeader}" />
                                    <Grid Margin="0 5 0 0">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="32" />
                                            <ColumnDefinition Width="*" />
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="32" />
                                        </Grid.RowDefinitions>

                                        <Button Grid.Column="0" Grid.Row="0" Tag="Bell" Style="{DynamicResource pp_settingsIconBox}" />

