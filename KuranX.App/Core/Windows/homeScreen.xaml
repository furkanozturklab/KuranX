                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="50" />
                            <RowDefinition Height="638" />
                        </Grid.RowDefinitions>
                        <Border  Grid.Row="0" Style="{DynamicResource pp_BorderHeader}">
