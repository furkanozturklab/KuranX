            <Popup x:Name="popup_settingsPage" IsOpen="False" Placement="Center" AllowsTransparency="True" Width="570" Height="690" VerticalAlignment="Center" HorizontalAlignment="Center" PopupAnimation="Fade">
                <Border Style="{DynamicResource defaultBorderShadowEffect}" Width="550" Height="670">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="50" />
                            <RowDefinition Height="*" />
                        </Grid.RowDefinitions>
                        <Border  Grid.Row="0" Style="{DynamicResource pp_BorderHeader}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="48" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="48" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="Program Ayarları" Style="{DynamicResource pp_HeaderText}" />
                                <Button Content="Move" Tag="MoveiOS" Uid="popup_settingsPage" Click="popuverMove_Click" Style="{DynamicResource pp_moveBaseIcon}" Background="Transparent" />
                            </Grid>
                        </Border>
                        <Grid Grid.Row="1">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="*" />
                                <RowDefinition Height="50" />
                            </Grid.RowDefinitions>
                            <Grid>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="70" />
                                    <RowDefinition Height="*" />
                                </Grid.RowDefinitions>
                                <settingsUI:ChangeButton changeType="switch_settings" x:Name="sw_setting"/>
                                <ScrollViewer Grid.Row="1" VerticalScrollBarVisibility="Disabled" HorizontalScrollBarVisibility="Disabled">
                                    <StackPanel x:Name="loadUserControll"  Orientation="Vertical" VerticalAlignment="Top" HorizontalAlignment="Stretch">
                                    </StackPanel>
                                </ScrollViewer>
                            </Grid>

                            <Grid Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center" Grid.Column="0">
                                    <Button x:Name="settingSave" Style="{DynamicResource pp_DefaultButtonBstrp}" Tag="Check2" Content="Kaydet" HorizontalAlignment="Center" Click="save_settings"/>
                                    <Button  Style="{DynamicResource pp_DefaultButtonClosed}" Tag="CloseBig" Content="Vazgeç" Uid="popup_settingsPage" HorizontalAlignment="Center"  Click="popupClosed_Click"/>
                                </StackPanel>
                            </Grid>
                        </Grid>
                    </Grid>
                </Border>
            </Popup>





            <Popup StaysOpen="true" x:Name="popup_ErrOpenPopup" IsOpen="False" Placement="Center" AllowsTransparency="True" Width="440" Height="540" VerticalAlignment="Center" HorizontalAlignment="Center" PopupAnimation="Fade">

                <Border Style="{DynamicResource defaultBorderShadowEffect}" Width="420" Height="520">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="50" />
                            <RowDefinition Height="470" />
                        </Grid.RowDefinitions>
                        <Border  Grid.Row="0" Style="{DynamicResource pp_BorderHeader}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="48" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="48" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="Hata Bildirimi" Style="{DynamicResource pp_HeaderText}" />
