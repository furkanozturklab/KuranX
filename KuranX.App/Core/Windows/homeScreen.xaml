                                </StackPanel>

                                <StackPanel Grid.Row="7" Grid.Column="0" Grid.ColumnSpan="4" Grid.RowSpan="4" Margin="20 5 20 5">
                                    <TextBlock Text="Mesaj"  Style="{DynamicResource prf_editBoxText}" />
