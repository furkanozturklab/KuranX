                                        <Border Width="10" Background="{Binding NoteLocation}" CornerRadius="5 0 0 5" />
                                        <Border Width="170" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="0 0 0 0">
                                            <StackPanel Orientation="Vertical" Margin="10 0 10 0">
                                                <TextBlock Text="{Binding NoteHeader}" FontSize="12" Style="{DynamicResource nt_ItemHeader}" />
                                                <TextBlock Text="{Binding Created}" Style="{DynamicResource nt_ItemDate}" />
                                            </StackPanel>
                                        </Border>
                                        <Border Width="24" Height="24" Margin="0 0 10 0">
                                            <Button Style="{DynamicResource nt_ItemOpenIcon}" Uid="{Binding NotesId}" Content="{Binding NoteHeader}" Tag="ArrowRight" Click="gotoNotes_Click" />
                                        </Border>
                                        <Border Width="24" Height="24" Margin="0 0 7 0">
                                            <Button Style="{DynamicResource nt_ItemOpenIcon}" Uid="{Binding NotesId}" Tag="Trash" Click="deleteNotes_Click" />
                                        </Border>
                                    </StackPanel>
                                </Border>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </Border>

