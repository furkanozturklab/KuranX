        </Popup>

        <Border Padding="10 5 10 5" Style="{DynamicResource aniEffectFadeBorder}" x:Name="loadBorder" Visibility="Hidden">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="100" />
                    <RowDefinition Height="70" />
                    <RowDefinition Height="490" />
                </Grid.RowDefinitions>
                <StackPanel Grid.Row="0" Orientation="Vertical" VerticalAlignment="Center">
                    <Border Style="{DynamicResource frameTab}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="10" />
                                <ColumnDefinition Width="*" />
                                <ColumnDefinition Width="250" />
                            </Grid.ColumnDefinitions>
                            <Border Grid.Column="0"  CornerRadius="5 0 0 5" x:Name="noteType" />

                            <StackPanel Grid.Column="1" Orientation="Vertical" Margin="10 5 10 5">
                                <TextBlock x:Name="header" Text="" Foreground="#1C1E2F" FontFamily="{DynamicResource PoppinsSemiBold}" FontSize="16" />
                                <TextBlock x:Name="create" Text="" Foreground="#7B7B7B" FontFamily="{DynamicResource PoppinsMedium}" FontSize="11" Margin="0 5 0 5" />
                                <TextBlock x:Name="location" Text="" Foreground="#C9283E" FontFamily="{DynamicResource PoppinsMedium}" FontSize="11" />
                            </StackPanel>
                            <StackPanel Grid.Column="2" Orientation="Horizontal" VerticalAlignment="Center">
                                <TextBlock TextWrapping="Wrap" TextAlignment="Center" Width="250" x:Name="infoText" Text="" Foreground="#C9283E" FontFamily="{DynamicResource PoppinsMedium}" FontSize="12" />
                            </StackPanel>
                        </Grid>
                    </Border>
                </StackPanel>
                <Grid Grid.Row="1"  Grid.ColumnSpan="2">

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <Grid.RowDefinitions>
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Button x:Name="gotoBackButton" Grid.Column="0" Style="{DynamicResource noteControlBackBtn}" Tag="ArrowLeft" Content="Geri Dön" Click="gotoBackButton_Click" />
                    <Button x:Name="gotoVerseButton" Grid.Column="1" Style="{DynamicResource noteControlBackGo}" Tag="ArrowRight" Content="Ayete Git" Click="gotoVerseButton_Click" />
                    <Button x:Name="sendResult" Grid.Column="2" Style="{DynamicResource noteControlBackSend}" Tag="Archive" Content="Sonuc" Click="sendResult_Click" />
                    <Button x:Name="printButton" Grid.Column="3" Style="{DynamicResource noteControlBackPrint}" Tag="Printer" Content="Yazdır" Click="printButton_Click" />
                    <Button x:Name="deleteButton" Grid.Column="4" Style="{DynamicResource noteControlBackDelete}" Tag="Trash" Content="Notu Sil" Click="deleteButton_Click" />
                    <Button x:Name="saveButton" Grid.Column="5" IsEnabled="False" Style="{DynamicResource noteControlBackSave}" Tag="Save" Content="Kaydet" Click="saveButton_Click" />
                </Grid>
                <Border Grid.Row="3" Grid.ColumnSpan="2" Padding="10" Background="#fff" Style="{DynamicResource defaultBorderShadowEffect}">

                    <TextBox x:Name="noteDetail" TextWrapping="Wrap" AcceptsReturn="True" MaxLength="3500" Style="{DynamicResource noteDetailTxtBox}" Text="" TextChanged="noteDetail_TextChanged" />
                </Border>
            </Grid>
        </Border>
    </Grid>
</Page>