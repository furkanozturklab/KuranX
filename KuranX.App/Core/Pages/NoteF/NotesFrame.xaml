                                                </StackPanel>
                                            </StackPanel>
                                        </Border>

                                        <Border Width="24" Height="24" Margin="0 0 7 0">
                                            <Button Style="{DynamicResource viewStackBtn}" Content="{Binding Count}" Uid="{Binding Fr}" Tag="{Binding Bg}" />
                                        </Border>

                                        <Border Width="24" Height="24" Margin="0 0 10 0">
                                            <Button Style="{DynamicResource fileItemOpenIcon}" Content="{Binding NoteLocation}" Tag="ArrowRight" Click="filter_Click" />
                                        </Border>
                                    </StackPanel>
                                </Border>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </Border>

                <Border Grid.Column="0" Grid.Row="2" VerticalAlignment="Center" Padding="10" HorizontalAlignment="Center">
                    <ItemsControl x:Name="nts4">
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>

                                <Border Width="245" Height="50" Style="{DynamicResource defaultBorderShadowEffect}">
                                    <StackPanel Orientation="Horizontal">
                                        <Border Width="15" Background="{Binding Bg}" CornerRadius="5 0 0 5" />
                                        <Border Width="170" HorizontalAlignment="Center" VerticalAlignment="Center" Padding="10 0 10 0">
                                            <StackPanel Orientation="Horizontal">
                                                <StackPanel Orientation="Vertical" Margin="10 0 10 0">
                                                    <TextBlock Text="{Binding NoteLocation}" FontSize="12" Style="{DynamicResource fileFolderHeader}" />
                                                </StackPanel>
                                            </StackPanel>
                                        </Border>

                                        <Border Width="24" Height="24" Margin="0 0 7 0">
                                            <Button Style="{DynamicResource viewStackBtn}" Content="{Binding Count}" Uid="{Binding Fr}" Tag="{Binding Bg}" />
                                        </Border>

                                        <Border Width="24" Height="24" Margin="0 0 10 0">
                                            <Button Style="{DynamicResource fileItemOpenIcon}" Content="{Binding NoteLocation}" Tag="ArrowRight" Click="filter_Click" />
                                        </Border>
                                    </StackPanel>
                                </Border>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </Border>
            </Grid>
        </StackPanel>

        <StackPanel Grid.Row="2" Grid.ColumnSpan="2" Orientation="Horizontal" HorizontalAlignment="Center" x:Name="loadFilterAni" Style="{DynamicResource aniEffectFadeStack}" Visibility="Visible">
            <StackPanel  Orientation="Horizontal">
                <StackPanel Style="{DynamicResource ayetlerControlItems}">
                    <Button x:Name="filterAll" IsEnabled="False" Tag="#343a40" Content="Hepsi" Style="{DynamicResource notepageColorControl}" Click="filter_Click" />
                </StackPanel>

                <StackPanel Style="{DynamicResource ayetlerControlItems}">
                    <Button x:Name="filterUser" IsEnabled="False" Tag="#ADB5BD" Content="Kullanıcı" Style="{DynamicResource notepageColorControl}" Click="filter_Click" />
                </StackPanel>

                <StackPanel Style="{DynamicResource ayetlerControlItems}">
                    <Button x:Name="filterSubject" IsEnabled="False" Tag="#FD7E14" Content="Konularım" Style="{DynamicResource notepageColorControl}" Click="filter_Click" />
                </StackPanel>

                <StackPanel Style="{DynamicResource ayetlerControlItems}">
                    <Button x:Name="filterLib" IsEnabled="False" Tag="#E33FA1" Content="Kütüphane" Style="{DynamicResource notepageColorControl}" Click="filter_Click" />
                </StackPanel>

                <StackPanel Style="{DynamicResource ayetlerControlItems}">
                    <Button x:Name="filterVerse" IsEnabled="False" Tag="#0DCAF0" Content="Ayet" Style="{DynamicResource notepageColorControl}" Click="filter_Click" />
                </StackPanel>

                <StackPanel Style="{DynamicResource ayetlerControlItems}">
                    <Button x:Name="filterPdf" IsEnabled="False" Tag="#B30B00" Content="PDF" Style="{DynamicResource notepageColorControl}" Click="filter_Click" />
                </StackPanel>
            </StackPanel>
        </StackPanel>

        <StackPanel Grid.Row="3" Grid.ColumnSpan="2" Orientation="Vertical" HorizontalAlignment="Right" x:Name="loadControlAni" Style="{DynamicResource aniEffectFadeStack}" Visibility="Visible">
            <StackPanel Orientation="Horizontal" Height="80">
                <StackPanel Style="{DynamicResource frameItemsOffsetNav}">
                    <Button IsEnabled="False" x:Name="previusPageButton" Style="{DynamicResource frameItemsControlNav}" Tag="AngleLeft" Content="Önceki Sayfa" Click="previusPageButton_Click" />
                </StackPanel>
                <StackPanel Style="{DynamicResource frameItemsOffsetNav}">
                    <Button IsEnabled="False" x:Name="nowPageStatus" Style="{DynamicResource frameItemsNumberNav}" Tag="1 / 1" Content="Sayfa" />
                </StackPanel>
                <StackPanel Style="{DynamicResource frameItemsOffsetNav}">
                    <Button IsEnabled="False" x:Name="totalcountText" Style="{DynamicResource frameItemsNumberNav}" Tag="-" Content="Notlar" />
                </StackPanel>
                <StackPanel Style="{DynamicResource frameItemsOffsetNav}">
                    <Button IsEnabled="False" x:Name="nextpageButton" Style="{DynamicResource frameItemsControlNav}" Tag="AngleRight" Content="Sonraki Sayfa" Click="nextpageButton_Click" />
                </StackPanel>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>