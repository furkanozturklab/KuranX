                                    <ComboBoxItem Style="{DynamicResource ResourceKey=ayetlerNavComboboxItems}" Content="Mekke" Name="Mekke" />
                                    <ComboBoxItem Style="{DynamicResource ResourceKey=ayetlerNavComboboxItems}" Content="Medine" Name="Medine" />
                                </ComboBox>
                            </StackPanel>

                            <StackPanel Margin="0,0,25,0" x:Name="deskingStack">
