                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>

                    <Border Grid.Column="0" Style="{DynamicResource ResourceKey=ayetlerPageItemNavBorder}" HorizontalAlignment="Stretch" VerticalAlignment="Center">
                        <StackPanel Orientation="Horizontal">
                            <RadioButton x:Name="allCheck" x:Uid="Tümü" Tag="All" Style="{DynamicResource ResourceKey=ayetlerPageItemNavButton}" BorderThickness="0,0,0,2" IsEnabled="True" IsChecked="True" Click="checkstatustype_click" />
                            <RadioButton x:Name="readCheck" x:Uid="Okuduklarım" Tag="Read" Style="{DynamicResource ResourceKey=ayetlerPageItemNavButton}" Click="checkstatustype_click" IsEnabled="True" />
